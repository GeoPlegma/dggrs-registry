    <!-- http://www.semanticweb.org/dggrs/aperture -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dggrs/aperture">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dggrs/DGGRS"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</xsd:minInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en">The number of zones in the subsequent DGGRS refinement 
                    level corresponding to a zone on the current level. Rate 
                    of zone sub-division. The higher the number, the ´faster&apos; 
                    a DGGRS refines to small zones.</rdfs:comment>
        <rdfs:label xml:lang="en">DGGRS zone sub-division aperture</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/dggrs/zoneShape -->


    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/dggrs/zoneShape">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/dggrs/DGGRS"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Usually a regular shape like the hexagon or triangle. 
                         May also be a rhombic shape such as the parellelogram.</rdfs:comment>
        <rdfs:label xml:lang="en">Shape of zones (cells) in a DGGRS</rdfs:label>
    </owl:DatatypeProperty>
    


