    <!-- http://www.semanticweb.org/dggrs/dodecahedron -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dggrs/dodecahedron">
        <rdf:type rdf:resource="http://www.semanticweb.org/dggrs/Polyhedron"/>
        <rdfs:comment xml:lang="en">A solid with twelve equal faces which are pentagons.</rdfs:comment>
        <rdfs:label xml:lang="en">Dodecahedron</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dggrs/gbt -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dggrs/gbt">
        <rdf:type rdf:resource="http://www.semanticweb.org/dggrs/Index"/>
        <rdfs:comment xml:lang="en">Indexes hexagonal grid systems of aperture 7. At 
                           each refinement level cells rotate circa 20º so that 
                           vertices of a cell septet concide with the six 
                           vertices of a cell in the previous refinement level.</rdfs:comment>
        <rdfs:label xml:lang="en">Generalised Balanced Trenary</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://en.wikipedia.org/wiki/Generalized_balanced_ternary"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dggrs/hexahedron -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dggrs/hexahedron">
        <rdf:type rdf:resource="http://www.semanticweb.org/dggrs/Polyhedron"/>
        <rdfs:comment xml:lang="en">A solid with six equal faces which are squares</rdfs:comment>
        <rdfs:label xml:lang="en">Hexahedron</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dggrs/icosahedron -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dggrs/icosahedron">
        <rdf:type rdf:resource="http://www.semanticweb.org/dggrs/Polyhedron"/>
        <rdfs:comment xml:lang="en">A solid with twenty equal faces which are equilateral 
                           triangles.</rdfs:comment>
        <rdfs:label xml:lang="en">Icosahedron</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dggrs/tetrahedron -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dggrs/tetrahedron">
        <rdf:type rdf:resource="http://www.semanticweb.org/dggrs/Polyhedron"/>
        <rdfs:comment xml:lang="en">A solid with four equal faces which are equilateral 
                           triangles.</rdfs:comment>
        <rdfs:label xml:lang="en">Tetrahedron</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dggrs/z7 -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dggrs/z7">
        <rdf:type rdf:resource="http://www.semanticweb.org/dggrs/Index"/>
        <rdfs:comment xml:lang="en">An index for hexagonal grid systems of aperture 7.</rdfs:comment>
        <rdfs:label xml:lang="en">Z7</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://dx.doi.org/10.5194/agile-giss-6-32-2025"/>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/dggrs/9r -->


    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/dggrs/9r">
        <rdf:type rdf:resource="http://www.semanticweb.org/dggrs/Index"/>
        <rdfs:comment xml:lang="en">A highly compact index for hexagonal grid systems 
