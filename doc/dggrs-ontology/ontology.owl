    <!-- http://www.semanticweb.org/dggrs/DGGRS -->


    <owl:Class rdf:about="http://www.semanticweb.org/dggrs/DGGRS">
        <rdfs:subClassOf rdf:resource="https://w3id.org/geosrsSpatialReferenceSystem"/>
        <rdfs:comment xml:lang="en">A Spatial Reference System composed of a Discrete Global 
                       Grid System and a zone (cell) indexing mechanism allowing 
                       for discrete but precise location on the surface of the 
                       Earth.</rdfs:comment>
        <rdfs:label xml:lang="en">Discrete Global Grid Reference System (DGGRS)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/dggrs/ImplDGGRS -->


    <owl:Class rdf:about="http://www.semanticweb.org/dggrs/ImplDGGRS">
        <rdfs:comment xml:lang="en">A software system implementing a DGGRS. It should at a minimum provide an API to transform a geographic location into a zone (cell) identifier at a given refinement level (resolution), plus its inverse. </rdfs:comment>
        <rdfs:label xml:lang="en">DGGRS implementation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/dggrs/Index -->


    <owl:Class rdf:about="http://www.semanticweb.org/dggrs/Index">
        <rdfs:comment xml:lang="en">A mathematical function, ordering mechanism, or any other 
                       method providing a unique identifier to each zone (cell) 
                       of a DGGRS, at any refinement level (resolution).</rdfs:comment>
        <rdfs:label xml:lang="en">DGGRS indexing mechanism</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/dggrs/Polyhedron -->


    <owl:Class rdf:about="http://www.semanticweb.org/dggrs/Polyhedron">
        <rdfs:comment xml:lang="en">A multi-faceted solid providing a geometric structure to 
                       divide a sphere or ellipsoid. The platonic solids are 
                       the most commonly used, although alternatives may also 
                       be used, such as the truncated icosahedron.</rdfs:comment>
        <rdfs:label xml:lang="en">Polyhedron</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/geosrsSpatialReferenceSystem -->


    <owl:Class rdf:about="https://w3id.org/geosrsSpatialReferenceSystem"/>
    


