}, {
  "@id" : "http://xmlns.com/foaf/0.1/name",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ]
}, {
  "@id" : "https://w3id.org/dggrs/9r",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "https://w3id.org/dggrs/Index"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "A highly compact index for hexagonal grid systems \n                        developed by Ecere and first implemented by DGGAL."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "9R"
  } ]
}, {
  "@id" : "https://w3id.org/dggrs/DGGRS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "A Spatial Reference System composed of a Discrete Global \n                       Grid System and a zone (cell) indexing mechanism allowing \n                       for discrete but precise location on the surface of the \n                       Earth."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Discrete Global Grid Reference System (DGGRS)"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://w3id.org/geosrsSpatialReferenceSystem"
  } ]
}, {
  "@id" : "https://w3id.org/dggrs/ImplDGGRS",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "A software system implementing a DGGRS. It should at a minimum provide an API to transform a geographic location into a zone (cell) identifier at a given refinement level (resolution), plus its inverse. "
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "DGGRS implementation"
  } ]
}, {
  "@id" : "https://w3id.org/dggrs/Index",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "A mathematical function, ordering mechanism, or any other \n                       method providing a unique identifier to each zone (cell) \n                       of a DGGRS, at any refinement level (resolution)."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "DGGRS indexing mechanism"
  } ]
}, {
  "@id" : "https://w3id.org/dggrs/Polyhedron",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "A multi-faceted solid providing a geometric structure to \n                       divide a sphere or ellipsoid. The platonic solids are \n                       the most commonly used, although alternatives may also \n                       be used, such as the truncated icosahedron."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Polyhedron"
  } ]
}, {
  "@id" : "https://w3id.org/dggrs/aperture",
  "@type" : [ "http://www.w3.org/2002/07/owl#DatatypeProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "The number of zones in the subsequent DGGRS refinement \n                    level corresponding to a zone on the current level. Rate \n                    of zone sub-division. The higher the number, the ´faster' \n                    a DGGRS refines to small zones."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "https://w3id.org/dggrs/DGGRS"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "DGGRS zone sub-division aperture"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "_:genid3"
  } ]
}, {
  "@id" : "https://w3id.org/dggrs/basePolyhedron",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Usually a polyhedron on which faces the datum \n                      surface is projected for further sub-division."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#domain" : [ {
    "@id" : "https://w3id.org/dggrs/DGGRS"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Base polyhedron of a DGGRS"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#range" : [ {
    "@id" : "https://w3id.org/dggrs/Polyhedron"
  } ]
}, {
  "@id" : "https://w3id.org/dggrs/dodecahedron",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "https://w3id.org/dggrs/Polyhedron"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "A solid with twelve equal faces which are pentagons."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Dodecahedron"
  } ]
}, {
  "@id" : "https://w3id.org/dggrs/gbt",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "https://w3id.org/dggrs/Index"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "Indexes hexagonal grid systems of aperture 7. At \n                           each refinement level cells rotate circa 20º so that \n                           vertices of a cell septet concide with the six \n                           vertices of a cell in the previous refinement level."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Generalised Balanced Trenary"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#seeAlso" : [ {
    "@id" : "https://en.wikipedia.org/wiki/Generalized_balanced_ternary"
  } ]
}, {
  "@id" : "https://w3id.org/dggrs/hexahedron",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "https://w3id.org/dggrs/Polyhedron"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "A solid with six equal faces which are squares"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Hexahedron"
  } ]
}, {
  "@id" : "https://w3id.org/dggrs/icosahedron",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "https://w3id.org/dggrs/Polyhedron"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "A solid with twenty equal faces which are equilateral \n                           triangles."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Icosahedron"
  } ]
}, {
  "@id" : "https://w3id.org/dggrs/implements",
  "@type" : [ "http://www.w3.org/2002/07/owl#ObjectProperty" ],
