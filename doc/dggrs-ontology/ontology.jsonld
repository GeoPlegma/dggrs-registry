  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "en",
    "@value" : "A means to express and record Discrete Global Grid \n                        Reference Systems on the web. This ontology depends on \n                        the CRS ontology in development at the OGC."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "en",
    "@value" : "Discrete Global Grid Reference Systems ontology"
  } ]
}, {
  "@id" : "http://www.w3.org/1999/02/22-rdf-syntax-ns#comment",
  "@type" : [ "http://www.w3.org/2002/07/owl#AnnotationProperty" ]
}, {
