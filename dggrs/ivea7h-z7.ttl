@prefix : <http://www.semanticweb.org/dggrs/ivea7h-z7> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix geosrs: <https://w3id.org/geosrs> .
@prefix dggrs: <http://www.semanticweb.org/dggrs/> .

:ivea7h-z7 a dggrs:DGGRS ;
    rdfs:label """IVEA7H-Z7"""@en ;
    rdfs:comment """Icosahedral equal-area slice & dice projection with great 
                    circle method. Hexagonal zone shape with reminiscent 
                    pentagons, refined with perture 7. Plus Z7 indexing 
                    mechanism."""@en ;
    dggrs:basePolyhedron dggrs:icosahedron ;
    geosrs:conversion geosrs:SliceAndDiceGreatCircle ;
    dggrs:zoneShape "hexagonal with 12 pentagons"@en ;
    dggrs:aperture "7"^^xsd:integer ;
    dggrs:usesIndex dggrs:Z7 .

